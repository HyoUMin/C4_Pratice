using System;

class Program
{
    static void Main() 
    {
        int[,] kor = new int[2, 3] { { 1, 2, 3 }, { 4, 5, 6 } };

        for (int i = 0; i < 2; i++)
        {
            for (int j = 0; j <= 2; j++)
            {
                Console.Write($"{kor[i, j]}_");
            }

            Console.WriteLine();
        }
    }
}
