using static System.Console;

class LAB
{
    static void Main()
    {
        double d = 12.34;
        int i = 1234;

        d = i;
        WriteLine(d);

        d = 12.34;
        i = (int)d;
        WriteLine("casting {0}", i);

        string s = "";
        s = Convert.ToString(d);
        WriteLine(s);
        WriteLine(s.GetType());
    }
}
